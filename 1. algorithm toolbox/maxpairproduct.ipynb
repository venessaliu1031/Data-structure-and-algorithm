   "source": [
    "n = int(input())\n",
    "a = [int(x) for x in input().split()]\n",
    "max1 = 0\n",
    "loc = 0\n",
    "max2 = 0\n",
    "\n",
    "\n",
    "for i, x in enumerate(a):\n",
    "    if x > max1: \n",
    "        max1 = x\n",
    "        loc = i\n",
    "for m, y in enumerate(a):\n",
    "    if y > max2 and y <= max1 and m !=loc:\n",
    "        max2 = y\n",
    "\n",
    "print(max1*max2)\n",
    "\n"
   ]
